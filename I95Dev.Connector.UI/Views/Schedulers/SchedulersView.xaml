                                FontWeight="SemiBold"
                                Text="{DynamicResource SchedulerDisable}" />
                        </Grid>

                        <Rectangle
                            Margin="0,5,0,0"
                            Grid.Row="6"
                            Grid.ColumnSpan="2"
                            Grid.Column="0"
                            HorizontalAlignment="Stretch"
                            Fill="{StaticResource BorderColor}"
                            Height="0.5" />

                        <Grid
                            Grid.Row="7"
                            Grid.ColumnSpan="2"
                            Grid.Column="0"
                            Style="{StaticResource DisabledGrid}"
                            IsEnabled="{Binding CanRun}">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition
                                    Width="1.25*" />
                                <ColumnDefinition
                                    Width="2*" />
                                <ColumnDefinition
                                    Width="3*" />
                            </Grid.ColumnDefinitions>
                            <TextBlock
                                Grid.Column="0"
                                Style="{StaticResource NormalTextBlock}"
                                FontWeight="SemiBold"
                                Text="{DynamicResource SchedulerRun}" />
                            <ToggleButton
                                Grid.Column="1"
                                Style="{StaticResource AnimatedSwitch}"
                                IsChecked="{Binding IsRunning, Mode=TwoWay}" />
                            <TextBlock
                                Grid.Column="2"
                                Style="{StaticResource NormalTextBlock}"
                                FontWeight="SemiBold"
                                Text="{DynamicResource SchedulerStop}" />
                        </Grid>
                    </Grid>
                </Grid>
            </Grid>

            <Grid
                Grid.Row="1"
                Grid.Column="1"
                DataContext="{Binding CleanupScheduler}"
                Margin="0,0,5,0">
                <Grid.RowDefinitions>
                    <RowDefinition
                        Height="35" />
                    <RowDefinition
                        Height="*" />
                </Grid.RowDefinitions>
                <TextBlock
                    Grid.Row="0"
                    Style="{StaticResource PageHeader}"
                    Text="{StaticResource SchedulerPanel5Heading}" />
                <StackPanel
                    Grid.Row="0"
                    Orientation="Horizontal"
                    Margin="0,0,10,0"
                    HorizontalAlignment="Right"
                    VerticalAlignment="Center">
                    <Button
                        Width="25"
                        Height="25"
                        Margin="5"
                        Command="{Binding CreateCommand}"
                        Style="{StaticResource BorderLessButton}">
                        <Rectangle
                            Margin="0"
                            Width="18"
                            Height="18"
                            Fill="{StaticResource BorderBrush}"
                            HorizontalAlignment="Right"
                            VerticalAlignment="Center">
                            <Rectangle.OpacityMask>
                                <VisualBrush
                                    Stretch="Uniform"
                                    Visual="{DynamicResource Plus}" />
                            </Rectangle.OpacityMask>
                        </Rectangle>
                    </Button>
                    <Button
                        Command="{Binding RefreshCommand}"
                        Width="26"
                        Height="25"
                        Style="{StaticResource BorderLessButton}">
                        <Rectangle
                            Margin="0"
                            Width="18"
                            Height="18"
                            Fill="{StaticResource BorderBrush}"
                            HorizontalAlignment="Right"
                            VerticalAlignment="Center">
                            <Rectangle.OpacityMask>
                                <VisualBrush
                                    Stretch="Uniform"
                                    Visual="{DynamicResource Reload}" />
                            </Rectangle.OpacityMask>
                        </Rectangle>
                    </Button>
                </StackPanel>
                <Grid
                    Grid.Row="1"
                    Background="{StaticResource PanelBackground}">
                    <Grid
                        Margin="10,5,10,5"
                        IsEnabled="{Binding IsCreated}"
                        Style="{StaticResource DisabledGrid}">
                        <Grid.RowDefinitions>
                            <RowDefinition
                                Height="Auto"
                                MinHeight="{StaticResource RowHeight}" />
                            <RowDefinition
                                Height="Auto"
                                MinHeight="{StaticResource RowHeight}" />
                            <RowDefinition
                                Height="Auto"
                                MinHeight="{StaticResource RowHeight}" />
                            <RowDefinition
                                Height="Auto"
                                MinHeight="{StaticResource RowHeight}" />
                            <RowDefinition
                                Height="Auto"
                                MinHeight="{StaticResource RowHeight}" />
                            <RowDefinition
                                Height="Auto"
                                MinHeight="{StaticResource RowHeight}" />
                            <RowDefinition
                                Height="Auto"
                                MinHeight="{StaticResource RowHeight}" />
                            <RowDefinition
                                Height="Auto"
                                MinHeight="{StaticResource RowHeight}" />
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition
                                Width="1.25*" />
                            <ColumnDefinition
                                Width="2*" />
                        </Grid.ColumnDefinitions>
                        <TextBlock
                            Grid.Row="0"
                            Grid.Column="0"
                            Style="{StaticResource TextBlockLeft}"
                            Text="{DynamicResource SchedulerName}" />
                        <TextBlock
                            Grid.Row="0"
                            Grid.Column="1"
                            Style="{StaticResource TextBlockRight}"
                            Text="{Binding SchedulerName}" />

                        <TextBlock
                            Grid.Row="1"
                            Grid.Column="0"
                            Style="{StaticResource TextBlockLeft}"
                            Text="{DynamicResource SchedulerStatus}" />
                        <TextBlock
                            Grid.Row="1"
                            Grid.Column="1"
                            Style="{StaticResource TextBlockRight}"
                            Text="{Binding Status}" />

                        <TextBlock
                            Grid.Row="2"
                            Grid.Column="0"
                            Style="{StaticResource TextBlockLeft}"
                            Text="{DynamicResource SchedulerLastRunTime}" />
                        <TextBlock
                            Grid.Row="2"
                            Grid.Column="1"
                            Style="{StaticResource TextBlockRight}"
                            Text="{Binding LastRuntime}" />

                        <TextBlock
                            Grid.Row="3"
                            Grid.Column="0"
                            Style="{StaticResource TextBlockLeft}"
                            Text="{DynamicResource SchedulerNextRunTime}" />
                        <TextBlock
                            Grid.Row="3"
                            Grid.Column="1"
                            Style="{StaticResource TextBlockRight}"
                            Text="{Binding NextRuntime}" />

                        <Rectangle
                            Margin="0,5,0,0"
                            Grid.Row="4"
                            Grid.ColumnSpan="2"
                            Grid.Column="0"
                            HorizontalAlignment="Stretch"
                            Fill="{StaticResource BorderColor}"
                            Height="0.5" />

                        <Grid
                            Grid.Row="5"
                            Grid.ColumnSpan="2"
                            Grid.Column="0">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition
                                    Width="1.25*" />
                                <ColumnDefinition
                                    Width="2*" />
                                <ColumnDefinition
                                    Width="3*" />
                            </Grid.ColumnDefinitions>
                            <TextBlock
                                Grid.Column="0"
                                Style="{StaticResource NormalTextBlock}"
                                FontWeight="SemiBold"
                                Text="{DynamicResource SchedulerEnable}" />
                            <ToggleButton
                                Grid.Column="1"
                                Style="{StaticResource AnimatedSwitch}"
                                IsChecked="{Binding IsEnabled, Mode=TwoWay}" />
                            <TextBlock
                                Grid.Column="2"
                                Style="{StaticResource NormalTextBlock}"
                                FontWeight="SemiBold"
                                Text="{DynamicResource SchedulerDisable}" />
                        </Grid>

                        <Rectangle
                            Margin="0,5,0,0"
                            Grid.Row="6"
                            Grid.ColumnSpan="2"
                            Grid.Column="0"
                            HorizontalAlignment="Stretch"
                            Fill="{StaticResource BorderColor}"
                            Height="0.5" />

                        <Grid
                            Grid.Row="7"
                            Grid.ColumnSpan="2"
                            Grid.Column="0"
                            Style="{StaticResource DisabledGrid}"
                            IsEnabled="{Binding CanRun}">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition
                                    Width="1.25*" />
                                <ColumnDefinition
                                    Width="2*" />
                                <ColumnDefinition
                                    Width="3*" />
                            </Grid.ColumnDefinitions>
                            <TextBlock
                                Grid.Column="0"
                                Style="{StaticResource NormalTextBlock}"
                                FontWeight="SemiBold"
                                Text="{DynamicResource SchedulerRun}" />
                            <ToggleButton
                                Grid.Column="1"
                                Style="{StaticResource AnimatedSwitch}"
                                IsChecked="{Binding IsRunning, Mode=TwoWay}" />
                            <TextBlock
                                Grid.Column="2"
                                Style="{StaticResource NormalTextBlock}"
                                FontWeight="SemiBold"
                                Text="{DynamicResource SchedulerStop}" />
                        </Grid>
                    </Grid>
                </Grid>
            </Grid>
        </Grid>
    </Grid>
</UserControl>